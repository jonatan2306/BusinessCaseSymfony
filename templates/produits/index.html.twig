{% extends 'base.html.twig' %}

{% block title %}Produits index{% endblock %}

{% block body %}
    <h1>Produits index</h1>
    <div class="container">
        <div class="row">
            <div class="products-container">
            <div class="product-row">
                <img src="data:image/jpeg;base64," alt="" >
                <div class="product-text-container">
                    <p>Granola gros éclat en chocolat 100g</p>
                </div>
                <div>
                    <p>3.99 €</p>
                </div>
                <div>
                    <button>Ajouter au panier</button>
                </div>
            </div>
        </div>
        </div>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Description</th>
                <th>PhotoProduit</th>
                <th>Prix</th>
                <th>Titre</th>
                <th>Quantite</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for produit in produits %}
            <tr>
                <td>{{ produit.id }}</td>
                <td>{{ produit.description }}</td>
                <td>{{ produit.photoProduit }}</td>
                <td>{{ produit.prix }}</td>
                <td>{{ produit.titre }}</td>
                <td>{{ produit.quantite }}</td>
                <td>
                    <a href="{{ path('app_produits_show', {'id': produit.id}) }}">show</a>
                            <a href="{{path("app_basket_add", {id: produit.id})}}" class="btn btn-success">
                        <i class="bi bi-cart-plus"></i> Ajouter
                    </a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
